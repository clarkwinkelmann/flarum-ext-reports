{
    "name": "clarkwinkelmann/flarum-ext-reports",
    "description": "Let users report content",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": ">=0.1.0-beta.15 <0.1.0-beta.16",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "homepage": "https://clarkwinkelmann.com/",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-reports"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\Reports\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Reports",
            "category": "feature",
            "icon": {
                "name": "fas fa-flag",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        }
    }
}
